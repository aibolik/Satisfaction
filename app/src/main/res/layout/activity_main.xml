<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start"
        android:id="@+id/start"
        android:onClick="playMusic"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="push me"
        android:id="@+id/pushme"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="playMusic"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="touch me"
        android:id="@+id/touchme"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="playMusic"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get mine"
        android:id="@+id/getmine"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="playMusic"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Satisfaction"
        android:id="@+id/satisfaction"
        android:onClick="playMusic"
        android:layout_below="@+id/touchme"
        android:layout_centerHorizontal="true"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="push me"
        android:id="@+id/fpushme"
        android:onClick="playMusic"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="touch me"
        android:id="@+id/ftouchme"
        android:onClick="playMusic"
        android:layout_alignTop="@+id/fpushme"
        android:layout_alignLeft="@+id/touchme"
        android:layout_alignStart="@+id/touchme"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get mine"
        android:id="@+id/fgetmine"
        android:onClick="playMusic"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/getmine"
        android:layout_alignStart="@+id/getmine"
        android:soundEffectsEnabled="false" />

    <kz.aibol.satisfaction.MyButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Satisfaction"
        android:id="@+id/fsatisfaction"
        android:onClick="playMusic"
        android:layout_below="@+id/ftouchme"
        android:layout_centerHorizontal="true"
        android:soundEffectsEnabled="false" />
</RelativeLayout>
